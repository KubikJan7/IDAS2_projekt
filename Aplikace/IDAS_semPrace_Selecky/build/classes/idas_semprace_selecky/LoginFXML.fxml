<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="261.0" prefWidth="400.0" styleClass="background" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
    <stylesheets>
        <URL value="@styl.css" />
    </stylesheets>
   <children>
      <TextField fx:id="usernameField" layoutX="147.0" layoutY="93.0" onKeyPressed="#inputCheck" promptText="Uživatel" />
      <PasswordField fx:id="passwordField" layoutX="147.0" layoutY="147.0" onKeyPressed="#inputCheck" promptText="Heslo" />
      <Text layoutX="60.0" layoutY="115.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="label-nadpisH2" text="Uživatel:" />
      <Text layoutX="62.0" layoutY="60.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="label-nadpisH1" text="Přihlášení uživatele" />
      <Text layoutX="60.0" layoutY="169.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="label-nadpisH2" text="Heslo:" />
      <Button fx:id="loginBtn" disable="true" layoutX="45.0" layoutY="200.0" mnemonicParsing="false" onAction="#prihlas" text="Přihlásit se" />
      <Button fx:id="vymazBtn" layoutX="170.0" layoutY="200.0" mnemonicParsing="false" onAction="#vycistiUdaje" text="Vymazat" />
      <Button fx:id="guestBtn" layoutX="288.0" layoutY="200.0" mnemonicParsing="false" onAction="#prihlasHosta" text="Guest" />
   </children>
</AnchorPane>
